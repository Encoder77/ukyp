<!DOCTYPE html>
<html lang="en">

<head>
  <!-- Required meta tags -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <title>Ukyp-Computers</title>
  <!-- plugins:css -->
  <link rel="stylesheet" href="/admin/vendors/feather/feather.css">
  <!-- inject:css -->
  <link rel="stylesheet" href="/admin/css/vertical-layout-light/style.css">
  <!-- endinject -->
  <link rel="shortcut icon" href="/admin/images/favicon.png" />
</head>

<body>
  <div class="container-scroller">
    <div class="container-fluid page-body-wrapper full-page-wrapper">
      <div class="content-wrapper d-flex align-items-center auth px-0">
        <div class="row w-100 mx-0">
          <div class="col-lg-4 mx-auto">
            <div class="auth-form-light py-5 px-4 px-sm-5">
              <b>ADMIN-Login</b>
              <h4>Ukyp-Computer Service</h4>
              <h6 class="fw-light">Sign in to continue.</h6>
              <form class="pt-3" onsubmit="return false;" id="form">
                <div class="form-group">
                  <input type="email" class="form-control form-control-lg" id="email" name="email" required placeholder="Email">
                </div>
                <div class="form-group">
                  <input type="password" class="form-control form-control-lg" id="password" name="password" required placeholder="Password">
                </div>
                <div id="error" class="alert alert-danger" style="display: none;">Error</div>
                <div id="success" class="alert alert-success" style="display: none;">Success</div>
                <div class="mt-3">
                  <button class="btn btn-block btn-primary btn-lg font-weight-medium auth-form-btn" type="submit">SIGN IN</button>
                </div>
              </form>
            </div>
          </div>
        </div>
      </div>
      <!-- content-wrapper ends -->
    </div>
    <!-- page-body-wrapper ends -->
  </div>
  <!-- container-scroller -->
  <script src="/js/login.js"></script>

  <!-- endinject -->
</body>

</html>
