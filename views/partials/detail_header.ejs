                    <ul>
                      <% for(let i=0; i < services.length; i++) {%>
                        <li><a href="/<%- lang %>/services/<%- services[i].id %>"><%- services[i].service_name %></a></li>
                        <% } %>
                    </ul>
                  </li>
                  <li> <a href="/<%- lang %>/blog" data-key="our-blog">Наш блог</a></li>
       
                  <li> <a href="/<%- lang %>/contact" data-key="contact">контакты</a><li>
                 
              
                </ul>
              </div>
