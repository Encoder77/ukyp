<header id="default_header" class="header_style_1">
    <!-- header top -->
    <div class="header_top">
      <div class="container">
        <div class="row">
          <div class="col-md-8">
            <div class="full">
              <div class="topbar-left">
                <ul class="list-inline">
                  <li> <span class="topbar-label">
                    <p style="color:white;"><a style="text-decoration:none;color:white;" href="tel:+99362912944">+99362912944</a></p>
                  </li>
                  <li>
                    <p  style="color:white;"><a style="text-decoration:none;color:white;" href="tel:+99364027628">+99364027628</a></p>
                  </li>
                  <li> <span class="topbar-label"><i class="fa fa-envelope-o"></i></span> <span class="topbar-hightlight"><a href="mailto:ukypcomputer@mail.ru">ukypcomputer@mail.ru</a></span> </li>
                </ul>
              </div>
            </div>
          </div>
          <div class="col-md-4 right_section_header_top">
          
            <div class="float-right d-flex flex-row justify-content-center align-center">
              <div class="d-flex align-center">
                <a onclick="langswitch('ru')" style="text-decoration:none;cursor: pointer;"><p class="mylangstyle">RU</p></a>
              </div>
              <div class="d-flex align-center">
                <a onclick="langswitch('tm')" style="text-decoration:none;cursor: pointer;"><p class="mylangstyle">TM</p></a>
            </div>
            </div>
  
          </div>
        </div>
      </div>
    </div>
    <!-- end header top -->
